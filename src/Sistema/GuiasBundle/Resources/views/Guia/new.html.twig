{% extends 'MWSimpleAdminCrudBundle:Default:new.html.twig' %}
    {% block page %}
        {% form_theme form 'SistemaGuiasBundle:Form:guias.theming.html.twig' %}
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="panel panel-primary center-block">
                    <div class="panel-heading">
                        <i class="glyphicon glyphicon-plus"></i> {{ 'views.new.creation'|trans({'%entity%': config.entityName}, 'MWSimpleAdminCrudBundle') }}
                    </div>
                    <div class="panel-body">
                        {% block form %}
                            {{ form(form) }}
                        {% endblock %}
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <a class="btn btn-info" href="{{ path(config.index) }}">
                    {{ 'views.recordactions.backtothelist'|trans({}, 'MWSimpleAdminCrudBundle') }}
                </a>
            </div>
         </div>
    {% endblock %}
{% block javascript %}
        {{ parent() }}
        {% javascripts '@SistemaGuiasBundle/Resources/public/js/*' %}
             <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
{% endblock %}